   function initialize() {
     var mapOptions = {
         center: new google.maps.LatLng(43.164, -87.977),
       zoom: 10,
       mapTypeId: google.maps.MapTypeId.ROADMAP
     };
     var map = new google.maps.Map(document.getElementById("map_canvas"),
       mapOptions);
     // create a marker
     var latlng = new google.maps.LatLng(43.164, -87.977);
     var marker = new google.maps.Marker({
       position: latlng,
       map: map,
       title: 'My Place'
     });

     var marker = new google.maps.Marker({
         position: { lat: 43.034, lng: -87.911 },
         map: map
     });


   }
</script> 
   

