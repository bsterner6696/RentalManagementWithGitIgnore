@model IEnumerable<RentalManagement.Models.Apartment>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RentalProperty.StreetAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RentPerMonth)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Unit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumberBedrooms)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumberBathrooms)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Features)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RentalProperty.StreetAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RentPerMonth)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Unit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumberBedrooms)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumberBathrooms)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Features)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

